<Bar Breakpoint="Breakpoint.Desktop" NavigationBreakpoint="Breakpoint.Tablet" ThemeContrast="ThemeContrast.Dark"
     Mode="BarMode.VerticalInline" CollapseMode="BarCollapseMode.Small">
    <BarToggler />
    <BarBrand>
        <BarItem>
            <BarLink To="">
                <BarIcon IconName="customIcon" />
                Clinic First
            </BarLink>
        </BarItem>
    </BarBrand>
    <BarMenu>
        <BarStart>
            <BarItem>
                <BarLink To="">
                    <BarIcon IconName="IconName.Dashboard" />
                    Dashboard
                </BarLink>
            </BarItem>
            <BarItem>
                <BarDropdown @bind-Visible="oriBlazorVisible">
                    <BarDropdownToggle>
                        <BarIcon IconName="IconName.Flag" />
                        Original Blazor Demo
                    </BarDropdownToggle>
                    <BarDropdownMenu>
                        <BarDropdownItem To="counter">Counter</BarDropdownItem>
                        <BarDropdownItem To="fetchdata">Fetch Data</BarDropdownItem>
                    </BarDropdownMenu>
                </BarDropdown>
            </BarItem>
            <BarItem>
                <BarDropdown @bind-Visible="uiElementsVisible">
                    <BarDropdownToggle>
                        <BarIcon IconName="IconName.Wrench" />
                        UI Elements
                    </BarDropdownToggle>
                    <BarDropdownMenu>
                        <BarDropdownItem To="uidemo/alerts">Alerts</BarDropdownItem>
                        <BarDropdownItem To="uidemo/badges">Badges</BarDropdownItem>
                        <BarDropdownItem To="uidemo/buttons">Buttons</BarDropdownItem>
                        <BarDropdownItem To="uidemo/breadcrumbs">Breadcrumbs</BarDropdownItem>
                        <BarDropdownItem To="uidemo/cards">Cards</BarDropdownItem>
                        <BarDropdownItem To="uidemo/carousel">Carousel</BarDropdownItem>
                        <BarDropdownItem To="uidemo/collapse">Collapse</BarDropdownItem>
                        <BarDropdownItem To="uidemo/dragdrop">Drag & Drop</BarDropdownItem>
                        <BarDropdownItem To="uidemo/dropdowns">Dropdowns</BarDropdownItem>
                        <BarDropdownItem To="uidemo/figures">Figures</BarDropdownItem>
                        <BarDropdownItem To="uidemo/file-picker">File Picker</BarDropdownItem>
                        <BarDropdownItem To="uidemo/icons">Icons </BarDropdownItem>
                        <BarDropdownItem To="uidemo/jumbotrons">Jumbotrons </BarDropdownItem>
                        <BarDropdownItem To="uidemo/listgroup">List group</BarDropdownItem>
                        <BarDropdownItem To="uidemo/modals">Modals</BarDropdownItem>
                        <BarDropdownItem To="uidemo/modal-provider">Modal Provider</BarDropdownItem>
                        <BarDropdownItem To="uidemo/paginations">Paginations</BarDropdownItem>
                        <BarDropdownItem To="uidemo/progressbars">Progress bars</BarDropdownItem>
                        <BarDropdownItem To="uidemo/ratings">Rating</BarDropdownItem>
                        <BarDropdownItem To="uidemo/steps">Steps</BarDropdownItem>
                        <BarDropdownItem To="uidemo/tables">Tables</BarDropdownItem>
                        <BarDropdownItem To="uidemo/tabs">Tabs</BarDropdownItem>
                        <BarDropdownItem To="uidemo/tooltips">Tooltips</BarDropdownItem>
                        <BarDropdownItem To="uidemo/typography">Typography</BarDropdownItem>


                    </BarDropdownMenu>
                </BarDropdown>
            </BarItem>
            <BarItem>
                <BarDropdown @bind-Visible="@formsVisible">
                    <BarDropdownToggle>
                        <BarIcon IconName="IconName.Edit" />
                        Forms
                    </BarDropdownToggle>
                    <BarDropdownMenu>
                        <BarDropdownItem To="uidemo/forms">
                            Forms
                        </BarDropdownItem>
                        <BarDropdownItem To="uidemo/focus-trap">
                            Focus Trap
                        </BarDropdownItem>
                        <BarDropdownItem To="uidemo/pickers">
                            Pickers
                        </BarDropdownItem>
                        <BarDropdownItem To="uidemo/validations">
                            Validations
                        </BarDropdownItem>
                        <BarDropdownItem To="uidemo/misc-forms">
                            Misc
                        </BarDropdownItem>
                        <BarDropdown>
                            <BarDropdownToggle>
                                Nested Forms
                            </BarDropdownToggle>
                            <BarDropdownMenu>
                                <BarDropdownItem To="uidemo/forms">
                                    Nested Forms
                                </BarDropdownItem>
                                <BarDropdownItem To="uidemo/validations">
                                    Nested Validations
                                </BarDropdownItem>
                            </BarDropdownMenu>
                        </BarDropdown>
                    </BarDropdownMenu>
                </BarDropdown>
            </BarItem>
            <BarItem>
                <BarDropdown @bind-Visible="@extensionsVisible">
                    <BarDropdownToggle>
                        <BarIcon IconName="IconName.PuzzlePiece" />
                        Extensions
                    </BarDropdownToggle>
                    <BarDropdownMenu>
                        <BarDropdownItem To="uidemo/autocomplete">
                            Autocomplete
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/components">
                            Components
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/charts">
                            Charts
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/charts/datalabels">
                            Charts Data Labels
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/livecharts">
                            Live Charts
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/loadingindicator">
                            Loading Indicator
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/markdown">
                            Markdown
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/treeview">
                            TreeView
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/richtextedit">
                            RichTextEdit
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/spinkit">
                            Spin Kit
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/video">
                            Video
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/qrcode">
                            QR Code
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/cropper">
                            Cropper
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/animate">
                            Animate
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/lottie-animation">
                            Lottie Animation
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/snackbar">
                            Snackbar
                        </BarDropdownItem>
                    </BarDropdownMenu>
                </BarDropdown>
            </BarItem>

            
        </BarStart>
        <BarEnd>
            <BarItem>
                <BarDropdown @bind-Visible="@utilitiesVisible">
                    <BarDropdownToggle>
                        <BarIcon IconName="IconName.Flask" />
                        Utilities
                    </BarDropdownToggle>
                    <BarDropdownMenu>
                        <BarDropdownItem To="tests/utilities/borders">
                            Borders
                        </BarDropdownItem>
                        <BarDropdownItem To="tests/utilities/flex">
                            Flex
                        </BarDropdownItem>
                        
                    </BarDropdownMenu>
                </BarDropdown>
            </BarItem>
            <BarItem>
                <BarDropdown>
                    <BarDropdownToggle>
                        <BarIcon IconName="IconName.Smartphone" />
                        Apps
                    </BarDropdownToggle>
                    <BarDropdownMenu>
                        <BarDropdownItem To="apps/todo">
                            Todo
                        </BarDropdownItem>
                    </BarDropdownMenu>
                </BarDropdown>
            </BarItem>
        </BarEnd>
    </BarMenu>
</Bar>
@code {
    private bool oriBlazorVisible = true;
    private bool uiElementsVisible = false;
    private bool utilitiesVisible = false;
    private bool extensionsVisible = false;
    private bool formsVisible = false;
    private bool datagridVisible = false;

    RenderFragment customIcon =@<img src="/cf-logo-no-text.png" style="width:32px; height: 32px" />;
}